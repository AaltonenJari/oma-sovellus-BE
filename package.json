{
  "name": "oma-sovellus-be",
  "version": "1.0.0",
  "description": "be testi",
  "license": "ISC",
  "author": "JA",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development node --watch index.js",
    "test": "cross-env  NODE_ENV=test node --test",
    "build:ui": "rimraf build && npm --prefix ../oma-sovellus install && npm --prefix ../oma-sovellus run build && npx cpy \"../oma-sovellus/build/**\" \"build\" --parents",
    "deploy:full": "npm run build:ui && git add build -f && git commit -m \"Update UI build\" && git push",
    "lint": "eslint ."
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.6.1",
    "cpy-cli": "^6.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "rimraf": "^6.1.2",
    "supertest": "^7.1.4"
  }
}
